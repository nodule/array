{
  "name": "fill",
  "ns": "array",
  "async": true,
  "description": "Fills an array to the length specified and outputs it.",
  "phrases": {
    "active": "Filling array"
  },
  "ports": {
    "input": {
      "in": {
        "title": "Value",
        "type": "any",
        "async": true
      },
      "length": {
        "title": "Length",
        "type": "number"
      },
      "array": {
        "title": "Array",
        "type": "array",
        "default": null
      }
    },
    "output": {
      "out": {
        "title": "Array",
        "type": "array"
      },
      "length": {
        "title": "New Length",
        "type": "number"
      }
    }
  }
}
